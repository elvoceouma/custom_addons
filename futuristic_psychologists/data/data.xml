<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences -->
        <record id="seq_clinical_psychologist_screening" model="ir.sequence">
            <field name="name">Clinical Psychologist Screening</field>
            <field name="code">clinical.psychologist.screening</field>
            <field name="prefix">SCR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_clinical_psychologist_session" model="ir.sequence">
            <field name="name">Clinical Psychologist Session</field>
            <field name="code">clinical.psychologist.session</field>
            <field name="prefix">CPS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_psychometric_assessment" model="ir.sequence">
            <field name="name">Psychometric Assessment</field>
            <field name="code">psychometric.assessment</field>
            <field name="prefix">PSY</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_advice_clinical_psychologist" model="ir.sequence">
            <field name="name">Advice Clinical Psychologist</field>
            <field name="code">advice.clinical.psychologist</field>
            <field name="prefix">ADV</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_discharge_summary" model="ir.sequence">
            <field name="name">Discharge Summary</field>
            <field name="code">discharge.summary</field>
            <field name="prefix">DS-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>