<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_hospital_case_history_form" model="ir.ui.view">
        <field name="name">hospital.case.history.form</field>
        <field name="model">hospital.case.history</field>
        <field name="arch" type="xml">
            <form>
                <header>
                    <button name="action_save" string="Save" type="object" class="oe_highlight"/>
                    <button name="action_discard" string="Discard" type="object"/>
                </header>
                <sheet>
                    <group>
                        <group>
                            <field name="patient_id" options="{'no_create': True}" widget="selection" string='Name'/>
                            <field name="age"/>
                            <field name="marital_status"/>
                            <field name="languages_known" widget="many2many_tags"/>
                            <field name="education"/>
                        </group>
                        <group>
                            <field name="dob"/>
                            <field name="sex"/>
                            <field name="religion"/>
                            <field name="other_religion" invisible="religion != 'other'"/>
                            <field name="socio_economic_status"/>
                            <field name="occupation"/>
                            <field name="vocation" invisible="occupation != 'specify_any'"/>
                        </group>
                    </group>

                    <group col="4" colspan="2" string="Informants">
                        <field name="informant_name"/>
                        <field name="relationship_id" widget="selection"/>
                        <field name="duration_stay"/>
                    </group>

                    <group string="INFORMATION : Reliable and Adequate">
                        <field name="information" widget="text_long"/>
                    </group>

                    <group string="CHIEF COMPLAINTS : - (Chronological order with duration)">
                        <field name="complaint_ids">
                            <tree>
                                <field name="chrono_no" string="Chrono. Order"/>
                                <field name="chief_compaints" string="Chief Complaints"/>
                                <field name="duration" string="Duration"/>
                                <field name="year" string="Year"/>
                            </tree>
                            <form>

                                <group>
                                    <field name="chrono_no" string="Chrono. Order"/>
                                    <field name="chief_compaints" string="Chief Complaints"/>
                                    <field name="duration" string="Duration"/>
                                    <field name="year" string="Year"/>
                                </group>

                            </form>
                        </field>
                    </group>

                    <group string="Onset of Illness: (Check appropriate box and describe about the onset)">
                        <field name="onset_of_ill" widget="radio"/>
                        <field name="abrupt_desc" invisible="onset_of_ill != 'Abrupt'"/>
                        <field name="acute_desc" invisible="onset_of_ill != 'Acute'"/>
                        <field name="subacute_desc" invisible="onset_of_ill != 'Sub-acute'"/>
                        <field name="insidious_desc" invisible="onset_of_ill != 'Insidious'"/>
                    </group>

                    <group>
                        <field name="course" placeholder="Describe the course of the illness..."/>
                    </group>

                    <group col="4" colspan="2" string="Duration of Illness (Specify Month or Years)">
                        <field name="duration_illness" widget="radio"/>
                        <field name="ill_months" invisible="duration_illness != 'Months'"/>
                        <field name="ill_years" invisible="duration_illness != 'Years'"/>
                    </group>

                    <group>
                        <field name="precipitating_factors" string=' Precipitating Factors (if any) ' widget='text_long'/>
                    </group>

                    <group string="HISTORY OF PRESENT ILLNESS (Describe Chief Complaints)">
                        <field name="history_present_ill" widget="text_long" />
                    </group>

                    <group string="Biological Functioning">
                        <group >
                            <field name="appetite" widget="radio"/>
                            <field name="bladder" widget="radio"/>
                            <field name="weight" widget="radio"/>
                        </group>
                        <group >
                            <field name="sleep" widget="radio"/>
                            <field name="bowel" widget="radio"/>
                            <field name="sexual" widget="radio"/>
                            <field name="other_sex_issues" invisible="sexual != 'Other issues'"/>
                        </group>
                    </group>

                    <group string="Social Functioning">
                        <field name="dress_take_care_himself" widget="radio"/>
                        <field name="goes_work" widget="radio"/>
                        <field name="does_husehold" widget="radio"/>
                        <field name="gets_along_wit_family" widget="radio"/>
                        <field name="gets_along_wit_neighbours" widget="radio"/>
                    </group>

                    <group string="Treatment History">
                            <field name="previous_hospitalization" widget='text_long'/>
                            <field name="opd" widget='text_long'/>
                            <field name="day_care_centre" widget='text_long'/>
                    </group>

                    <group string="Interval functioning (what is the patient like between episodes/when 'well'):">
                        <field name="interval_functioning" widget='text_long' />
                    </group>

                    <group string="Suicide attempts (If present, refer Suicidal Risk Assessment - Becks Suicide Intent Scale)/drug and alcohol abuse">
                        <field name="suicide_attempts" widget='text_long'/>
                    </group>

                    <group string="2. Medical Treatment History">
                        <label for="physical_illness" string="a. Physical Illness (Present)"/>
                        <field name="physical_illness" nolabel="1" widget='long_text'/>
                        <group>
                            <field name="head_injury" widget="radio"/>
                            <field name="specify_injury" invisible="head_injury != 'Present'"/>
                            <field name="past_surgeris" widget="radio"/>
                            <field name="specify_surgeries" invisible="past_surgeris != 'Present'"/>
                            <field name="seizure" widget="radio"/>
                            <field name="specify_seizure" invisible="seizure != 'Present'"/>
                        </group>
                        <group>
                            <field name="substance_use" widget="radio"/>
                            <field name="specify_substance" invisible="substance_use != 'Present'"/>
                            <field name="epilepsy" widget="radio"/>
                            <field name="specify_episodes" invisible="epilepsy != 'Present'"/>
                        </group>
                        <label for="allergies" string=" f.Allergies (to food, medications etc.,- specify) "/>
                            <field name="allergies" widget='long_text'/>
                    </group>

                    <group string="Check if the client has or had any of these issues:"/>
                    <group>
                        <group  string="Constitutional">
                            <field name="weight_loss" widget="radio"/>
                            <field name="fatigue" widget="radio"/>
                            <field name="fever" widget="radio"/>
                        </group>
                        <group  string="Respiratory">
                            <field name="cough_easy" widget="radio"/>
                            <field name="cough_blood" widget="radio"/>
                            <field name="wheezing" widget="radio"/>
                            <field name="chills" widget="radio"/>
                        </group>
                        <group  string="Hematology/Lymph">
                            <field name="easy_bruising" widget="radio"/>
                            <field name="gums_bleed_easily" widget="radio"/>
                            <field name="enlarged_glands" widget="radio"/>
                        </group>
                        <group  string="Eyes">
                            <field name="glass_contact" widget="radio"/>
                            <field name="eye_pain" widget="radio"/>
                            <field name="double_vision" widget="radio"/>
                            <field name="cataracts" widget="radio"/>
                        </group>
                        <group  string="Gastrointestinal">
                            <field name="heartburn_reflux" widget="radio"/>
                            <field name="nausea_vomiting" widget="radio"/>
                            <field name="constipation" widget="radio"/>
                            <field name="change_in_bm" widget="radio"/>
                            <field name="diarrhea" widget="radio"/>
                            <field name="jauntice" widget="radio"/>
                            <field name="abdominal_pain" widget="radio"/>
                            <field name="black_bloody_bm" widget="radio"/>
                        </group>
                        <group  string="Musculoskeletal">
                            <field name="joint_pain" widget="radio"/>
                            <field name="stiffness" widget="radio"/>
                            <field name="muscle_pain" widget="radio"/>
                            <field name="back_pain" widget="radio"/>
                        </group>
                        <group  string="Skin">
                            <field name="rash_shores" widget="radio"/>
                            <field name="lesions" widget="radio"/>
                            <field name="itching_burning" widget="radio"/>
                        </group>
                        <group  string="Ears,Nose,Throat">
                            <field name="difficulty_hearing" widget="radio"/>
                            <field name="ringing_ears" widget="radio"/>
                            <field name="vertigo" widget="radio"/>
                            <field name="sinus_trouble" widget="radio"/>
                            <field name="nasal_stuffiness" widget="radio"/>
                            <field name="freq_sore_throat" widget="radio"/>
                        </group>
                        <group  string="Genitourinary">
                            <field name="burning_freq" widget="radio"/>
                            <field name="nighttime" widget="radio"/>
                            <field name="blood_in_urine" widget="radio"/>
                            <field name="erictile_dysfunction" widget="radio"/>
                            <field name="abnormal_discharge" widget="radio"/>
                            <field name="bladder_leakage" widget="radio"/>
                        </group>
                        <group  string="Neurological">
                            <field name="loss_strength" widget="radio"/>
                            <field name="numbness" widget="radio"/>
                            <field name="headaches" widget="radio"/>
                            <field name="tremors" widget="radio"/>
                            <field name="memory_loss" widget="radio"/>
                        </group>
                        <group  string="Cardiovascular">
                            <field name="murmur" widget="radio"/>
                            <field name="chest_pain" widget="radio"/>
                            <field name="palpitations" widget="radio"/>
                            <field name="dizziness" widget="radio"/>
                            <field name="fainting_spells" widget="radio"/>
                            <field name="shortness_of_breathe" widget="radio"/>
                            <field name="difficult_lying_flat" widget="radio"/>
                            <field name="swelling_ankles" widget="radio"/>
                        </group>
                        <group  string="Endocrine">
                            <field name="loss_of_hair" widget="radio"/>
                            <field name="heat_cold_intolerance" widget="radio"/>
                        </group>
                        <group  string="Allergic/Immunologic">
                            <field name="hives_eczema" widget="radio"/>
                            <field name="hay_fever" widget="radio"/>
                        </group>
                        <group  string="Females Only">
                            <field name="last_date_mammogram"/>
                            <field name="mammogram_options" widget="radio"/>
                            <field name="date_last_pap"/>
                            <field name="pap_options" widget="radio"/>
                            <field name="age_onset_periods"/>
                            <field name="age_onset_menopause"/>
                            <field name="regular_periods" widget="radio"/>
                            <field name="no_pregnancies"/>
                        </group>
                    </group>

                    <group string="Psychiatric Medication">
                        <field name="physiatric_medication_ids" >
                            <tree editable="bottom">
                                <field name="name"/>
                                <field name="dosage"/>
                                <field name="frequency"/>
                                <field name="administration"/>
                                <field name="prescriber_name"/>
                            </tree>
                            <form string="Psychiatric Medication">
                                <group>
                                    <field name="name" options="{'no_open': True, 'no_create': True}"/>
                                    <field name="dosage"/>
                                    <field name="frequency"/>
                                    <field name="administration"/>
                                    <field name="prescriber_name"/>
                                </group>    
                            </form>
                        </field>
                    </group>

                    <group string="General Medication">
                        <field name="general_medication_ids" >
                            <tree editable="bottom">
                                <field name="name"/>
                                <field name="dosage"/>
                                <field name="frequency"/>
                                <field name="administration"/>
                                <field name="prescriber_name"/>
                            </tree>
                        </field>
                    </group>

                    <group string="Psychiatric Medication Effects">
                        <field name="physiatric_effect_ids" >
                            <tree editable="bottom">
                                <field name="name"/>
                                <field name="side_effects"/>
                                <field name="actions_taken"/>
                            </tree>
                        </field>
                    </group>

                    <group string="General Medication Effects">
                        <field name="general_effect_ids" >
                            <tree editable="bottom">
                                <field name="name"/>
                                <field name="side_effects"/>
                                <field name="actions_taken"/>
                            </tree>
                        </field>
                    </group>

                    <group string="History of Medicines Prescribed:">
                        <field name="history_medicines" nolabel="1" widget='long_text'/>
                    </group>

                    <group string="FAMILY HISTORY"/>
                    <group  string="1. Consanguinity in parents">
                        <field name="consanguinity_in_parents" />
                        <field name="consanguinity_specify" invisible="consanguinity_in_parents != 'Present'"/>
                    </group>
                    <group  string="2. Family history of psychiatric and physical illness (Suicide, Substance abuse, Epilepsy, Mental retardation etc)">
                        <field name="family_history_pp" />
                        <field name="present_specify" invisible="family_history_pp != 'Present'"/>
                    </group>
                    <group  string="3.Interpersonal Relations">
                        <!-- <field name="interpersonal_relations" /> -->
                        <field name="family_comm"/>
                        <field name="leadership_pattern"/>
                        <field name="role_functions"/>
                        <field name="social_support"/>
                        <field name="reinforcement"/>
                    </group>
                    <group string="4.Type of Family">
                        <field name="type_family" />
                    </group>
                    <group string="5.Family Environment">
                        <field name="family_environ" />
                    </group>

                    <group string="Personal History (Check if there were any issues during the period mentioned)"/>
                    <group  string="1.Birth">
                        <group string="Prenatal">
                            <field name="drug_exposure"/>
                            <field name="medical_complication"/>
                        </group>
                        <group string="Perinatal">
                            <field name="complication_during_preg"/>
                        </group>
                        <group string="Postnatal">
                            <field name="major_illness"/>
                        </group>
                        <group string="Delivery when mother was above the age of 30">
                            <field name="whether_client_first_child"/>
                        </group>
                    </group>
                    <group string="2.Early Development: (Mile stones of development, Behaviour problems and Physical illness)"/>
                    <group >
                        <field name="delayed_milestones" widget='long_text'/>
                        <field name="behaviour_probs" widget='long_text'/>
                        <field name="physical_illness_ids" widget="many2many_tags"/>
                    </group>
                    <group string="3.Childhood / Academic History [Peer group interaction, Physical Abuse, Substance Use]"/>
                    <group>
                        <field name="history_bullying" widget="checkbox"/>
                        <field name="check_boarding" widget="checkbox"/>
                    </group>
                    <group col="4" colspan="2">
                        <field name="parenting_style" widget="radio"/>
                        <field name="academic_performance" widget="radio"/>
                    </group>
                    <group string="4.Menstrual History">
                        <field name="menstrual_history"  widget='long_text'/>
                    </group>
                    <group string="5.Adolescence">
                        <field name="relationship_with_family" widget="radio"/>
                        <field name="specify_relationship" string="Specify if it was not good)" invisible="relationship_with_family not in ['Poor', 'Average']"/>
                        <field name="substance_usage"/>
                        <field name="bullying_history"/>
                        <field name="check_if_boarding"/>
                        <field name="parenting_style_adol" widget="radio"/>
                        <field name="academic_performance_adol" widget="radio"/>
                        <field name="specify_not_good"/>
                    </group>
                    <group string="6.Occupational History">
                        <field name="interaction_with_coll" widget="radio"/>
                        <field name="interaction_specify"/>
                        <field name="consistency_work" widget="radio"/>
                        <field name="consistency_specify"/>
                        <field name="other_issues"/>
                    </group>
                    <group col="4" colspan="2" string="7.Sexual History ( get these information in private keeping confidentiality)">
                        <field name="premarital_relationship" widget="radio"/>
                        <field name="history_masturbation" widget="radio"/>
                        <field name="if_yes_premarital"/>
                        <field name="if_yes_masturbation"/>
                        <field name="homosexuality" widget="radio"/>
                    </group>
                    <group col="4" colspan="2" string="8.Marital History">
                        <field name="relation_wife" widget="radio"/>
                        <field name="extra_marital" widget="radio"/>
                        <field name="if_no_good_relationship"/>
                        <field name="specify_if_extra"/>
                    </group>
                    <group string="PRE-MORBID Personality:  (Client's details i.e. before onset of illness)">
                        <group string="Social Relations: (Attachment/ Dependence to family, friends, groups, society, work and workmates)"/>
                        <newline/>
                        <field name="social_relations" nolabel="1"/>
                        <newline/>
                        <group string="Moral and religious Standards"/>
                        <newline/>
                        <field name="moral_religious" nolabel="1"/>
                        <newline/>
                        <group string="Intellectual Activities: (Hobbies and Interest)"/>
                        <newline/>
                        <field name="intellectual_activity" nolabel="1"/>
                        <newline/>
                        <group string="Predominant Mood: (Cheerful, Optimistic, Pessimistic, Stable, Fluctuating etc)"/>
                        <newline/>
                        <field name="predominant_mood" nolabel="1"/>
                        <newline/>
                        <group string="Character: (Attitude to work and responsibility, Interpersonal relationship, Standards in moral, religious)"/>
                        <newline/>
                        <field name="character" nolabel="1"/>
                        <newline/>
                        <group string="Use of Leisure Time:"/>
                        <newline/>
                        <field name="use_of_leisure" nolabel="1"/>
                        <newline/>
                        <group string="Habits: (Smoking, Alcohol, Chemicals, Panparang, Gutka, Panmasala, etc)"/>
                        <newline/>
                        <field name="habits" nolabel="1"/>
                        <newline/>
                        <group string="If any, specify in detail: (Mention the method of intake):"/>
                        <newline/>
                        <field name="if_any_special_detail" nolabel="1"/>
                    </group>
                    <group  string="Any Legal Involments: (If yes, details)">
                        <field name="present_involvment"/>
                        <field name="past_involvment"/>
                    </group>
                    <group string="Expectations of the Family (purpose of admission in the order of priority)">
                        <field name="expectations_family" nolabel="1"/>
                    </group>
                    <group string="Additional Remarks: (Escaping/ Suicidal/ Homicidal/ Aggressive/ Destructive/ if any others, specify)">
                        <field name="additional_remarks" nolabel="1"/>
                    </group>
                    <group string="Risk Assessment Information instead of Suicidal tendency (Attempt)">
                        <field name="risk_assessment_ids">
                            <tree>
                                <field name="name"/>
                                <field name="never"/>
                                <field name="past_week_month"/>
                                <field name="past_month_year"/>
                                <field name="past_2_years"/>
                                <field name="details" string=' 	
Please provide specific details of each of them'/>
                            </tree>
                            <form string="Risk Assessment">
                                <group>
                                    <field name="name" options="{'no_open': True, 'no_create': True}"/>
                                    <field name="never"/>
                                    <field name="past_week_month"/>
                                    <field name="past_month_year"/>
                                    <field name="past_2_years"/>
                                    <field name="details"/>
                                </group>
                            </form>
                        </field>
                    </group>

                    <!-- <field name="suicidal_tendency" invisible="1"/>
