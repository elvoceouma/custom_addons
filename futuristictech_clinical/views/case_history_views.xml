                    <group string="Mental Status Examination"/>
                    <group string="1.General Appearance and Behaviour:">
                        <field name="general_appearance"  widget="radio"/>
                    </group>

                    <group col="4" colspan="2">
                        <field name="dress" widget="radio"/>
                        <field name="eye_contact" widget="radio"/>
                        <field name="touch_with_surrounding" widget="radio"/>
                        <field name="rapport" widget="radio"/>
                        <field name="attitude_to_interviewer" widget="radio"/>
                    </group>

                    <group string="Psycomotot Activity">
                        <field name="psychomotor_activity_ids" nolabel="1" widget="many2many_tags"/>
                    </group>

                    <group string="Catatonic signs">
                        <field name="catatonic_signs" nolabel="1" widget="many2many_tags"/>
                    </group>

                    <group  string="Speech">
                        <field name="speech_initiation" widget="radio"/>
                        <field name="reaction_time" widget="radio"/>
                        <field name="intensity" widget="radio"/>
                        <field name="pitch" widget="radio"/>
                        <field name="speed" widget="radio"/>
                        <field name="pressure_speech" widget="radio"/>
                        <field name="volume" widget="radio"/>
                        <field name="relevance" widget="radio"/>
                        <field name="coherence" widget="radio"/>
                        <field name="deviation" widget="radio"/>
                    </group>

                    <group string="4.Thought">
                        <group string="Form">
                            <group string="Check for Formal thought disorder;"/>
                            <newline/>
                            <field name="check_for_disorder" nolabel="1" widget="radio"/>
                        </group>
                        <group col="4" colspan="2" string="Flow/Stream of thought">
                            <field name="flow_stream_normal" widget="radio"/>
                            <field name="flow_stream_abnormal" widget="radio"/>
                            <field name="relationship_in_patient" widget="radio"/>
                            <field name="blocking_distractibility" widget="radio"/>
                        </group>
                        <group string="Possession">
                            <field name="possession" nolabel="1" widget="radio"/>
                            <group string="Obsessions: elicit their nature">
                                <field name="obsession" nolabel="1" widget="radio"/>
                            </group>
                            <group string="Compulsions: nature of compulsion">
                                <field name="compulsions" nolabel="1" widget="radio"/>
                            </group>
                            <group string="Thought Alienation">
                                <field name="thought_alienation" nolabel="1" widget="radio"/>
                            </group>
                        </group>
                    </group>

                    <group string="Content">
                        <group string="Delusions">
                            <field name="delusions_prior" nolabel="1" widget="radio"/>
                        </group>
                        <group>
                            <field name="delusion_persecutory" widget="radio" />
                        </group>
                        <group>
                            <field name="delusion" widget="radio"/>
                        </group>
                        <group>
                            <field name="grandiose" widget="radio"/>
                        </group>
                        <group>
                            <field name="delusion_list" widget="radio"/>
                        </group>
                        <group>
                            <field name="conviction_validity" widget="radio"/>
                        </group>
                        <group>
                            <field name="how_affects_life" widget="radio"/>
                        </group>
                        <group string="Depressive Cognition">
                            <field name="depressive_cognition" nolabel="1" widget="radio"/>
                            <newline/>
                            <field name="depressive_overhauled" widget="radio"/>
                        </group>
                        <group string="Preoccupation">
                            <field name="preoccupation" nolabel="1" widget="radio"/>
                        </group>
                        <group>
                            <field name="preoccupation_list" nolabel="1" widget="radio"/>
                        </group>
                        <group string="Somatisation">
                            <field name="somatisation" nolabel="1"/>
                        </group>
                        <group string="Describe hallucinations:">
                            <field name="thought_sample" nolabel="1"/>
                        </group>
                    </group>

                    <group string="5.Mood">
                        <group string="Subjective">
                            <field name="subjective" widget="radio" nolabel="1"/>
