<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_medical_hospital" model="ir.sequence">
        <field name="name">Hospital Reference</field>
        <field name="code">medical.hospital</field>
        <field name="prefix">HOSP/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_medical_patient" model="ir.sequence">
        <field name="name">Patient Registration</field>
        <field name="code">medical.patient</field>
        <field name="prefix">PAT/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_medical_case" model="ir.sequence">
        <field name="name">Medical Case</field>
        <field name="code">medical.case</field>
        <field name="prefix">CASE/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_medical_treatment" model="ir.sequence">
        <field name="name">Medical Treatment</field>
        <field name="code">medical.treatment</field>
        <field name="prefix">TRT/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_medical_prescription" model="ir.sequence">
        <field name="name">Medical Prescription</field>
        <field name="code">medical.prescription</field>
        <field name="prefix">PRES/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_medical_payment" model="ir.sequence">
        <field name="name">Medical Payment</field>
        <field name="code">medical.payment</field>
        <field name="prefix">PAY/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <!-- Sequence for Clinical Trail -->
    <record id="seq_medical_clinical_trail" model="ir.sequence">
        <field name="name">Medical Clinical Trail</field>
        <field name="code">medical.clinical.trail</field>
        <field name="prefix">CT/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequence for Medical Consultation -->
    <record id="seq_medical_consultation" model="ir.sequence">
        <field name="name">Medical Consultation</field>
        <field name="code">medical.consultation</field>
        <field name="prefix">CONS/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_crm_lead_reference" model="ir.sequence">
        <field name="name">CRM Lead Reference</field>
        <field name="code">crm.lead.reference</field>
        <field name="prefix">REF/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_consultation_consultation" model="ir.sequence">
        <field name="name">Consultation</field>
        <field name="code">consultation.consultation</field>
        <field name="prefix">CN/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

     <!-- Sequence for Slot Booking -->
    <record id="seq_slot_booking" model="ir.sequence">
        <field name="name">Slot Booking Sequence</field>
        <field name="code">slot.booking</field>
        <field name="prefix">APT/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
        <field name="use_date_range">True</field>
    </record>

    <!-- Sequence for Available Slot -->
    <record id="seq_available_slot" model="ir.sequence">
        <field name="name">Available Slot Sequence</field>
        <field name="code">available.slot</field>
        <field name="prefix">TS/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
        <field name="use_date_range">True</field>
    </record>

    <!-- Sequence for Cancellation Request -->
    <record id="seq_cancellation_request" model="ir.sequence">
        <field name="name">Cancellation Request Sequence</field>
        <field name="code">cancellation.request</field>
        <field name="prefix">CR/%(year)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
        <field name="use_date_range">True</field>
    </record>
</odoo>